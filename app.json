{
  "expo": {
    "name": "LaPremiere",
    "slug": "lpl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "permissions": ["NOTIFICATIONS"]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "networking": {
      "whitelist":
      "http://118.67.143.125:8000/**"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gunho1212.lpl",
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a953e14a-6fe9-4a77-9546-5c2ce4e9d11c"
      }
    },
    "owner": "gunho1212",
    "plugins": [
      "expo-build-properties",
      "expo-font",
      "expo-build-properties"
    ]
  }
}
